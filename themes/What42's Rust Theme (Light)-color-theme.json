	/*
	"semanticHighlighting": true,

	"semanticTokenColors": {
		"*.documentation": "#0080C0",
		"*.injected": "#204060",
		"*.intraDocLink": "#7090FF",
		"*.unsafe": "#E00000",
		"attribute": "#0040C0",
		"attributeBracket": "#001080",
		"decorator": "#0070B0",
		"formatSpecifier": "#006030",
		"escapeSequence": "#D01080",
		"lifetime": "#E0B000",
		"selfKeyword": "#B02020",
		"selfTypeKeyword": "#B02020",
	},
	*/



	"tokenColors": [



		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#208010"
			}
		},
		{
			"name": "Documentation",
			"scope": "comment.line.documentation",
			"settings": {
				"foreground": "#0060F0"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type"
			],
			"settings": {
				"foreground": "#001070"
			}
		},
		{
			"name": "mut",
			"scope": "storage.modifier.mut.rust",
			"settings": {
				"foreground": "#881030"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#202090"
			}
		},
		{
			"name": "Functions",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#0418C0"
			}
		},
		{
			"name": "Types",
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#005460"
			}
		},
		{
			"name": "Self",
			"scope": "variable.language.self",
			"settings": {
				"foreground": "#8C0820"
			}
		},
		{
			"name": "Numbers & Characters",
			"scope": "constant",
			"settings": {
				"foreground": "#007070"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"markup.inline.raw.string",
			],
			"settings": {
				"foreground": "#700880"
			}
		},
		{
			"name": "Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#C01080"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#C01080"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.other.caps"
			],
			"settings": {
				"foreground": "#704838"
			}
		},
		{
			"name": "Macros",
			"scope": "meta.attribute",
			"settings": {
				"foreground": "#0420E0",
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#FF0000"
			}
		},



		{
			"name": "Toml Key",
			"scope": "keyword.key.toml",
			"settings": {
				"foreground": "#000000",
			}
		},
		{
			"name": "Toml Tag",
			"scope": "meta.tag.table.toml",
			"settings": {
				"foreground": "#0830D0",
				"fontStyle": "bold"
			}
		},



		{
			"name": "JSON Strings",
			"scope": "string.json",
			"settings": {
				"foreground": "#000000"
			}
		},



		{
			"name": "Headings",
			"scope": "heading",
			"settings": {
				"foreground": "#0030E0",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Links",
			"scope": "meta.link",
			"settings": {
				"foreground": "#0070D0",
			}
		},
		{
			"name": "Link Names",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#6000F0",
			}
		}



	]
}
